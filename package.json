{
  "name": "time-input-formatter",
  "description": "Interpret and format a user inputted time string or partial time string to 12hr or 24hr time",
  "scripts": {
    "compile": "npm run lint && rm -rf dist/lib && tsc && tsc --build tsconfig.es5.json && npm run readme",
    "lint": "eslint . --ext .ts",
    "readme": "node bin/generateReadme.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSpicyMeatball/time-input-formatter.git"
  },
  "keywords": [
    "javascript",
    "time",
    "input",
    "formatter",
    "formatting"
  ],
  "author": "Michael Paravano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheSpicyMeatball/time-input-formatter/issues"
  },
  "homepage": "https://github.com/TheSpicyMeatball/time-input-formatter#readme",
  "devDependencies": {
    "@paravano/utils": "^1.4.0",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "coveralls": "^3.1.0",
    "date-fns": "^2.21.1",
    "ejs": "^3.1.5",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "js-htmlencode": "^0.3.0",
    "jsdoc-parse-plus": "^1.3.0",
    "nyc": "^15.1.0",
    "typescript": "^4.1.3"
  }
}
